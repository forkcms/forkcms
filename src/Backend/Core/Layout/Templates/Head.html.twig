<!DOCTYPE html>
<html lang="{{ INTERFACE_LANGUAGE }}">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="chrome=1" />
  <meta name="robots" content="noindex, nofollow" />

  <title>{% if page_title %}{{ page_title|capitalize }} - {% endif %}{{ SITE_TITLE }} - Fork CMS</title>
  <link rel="shortcut icon" href="/src/Backend/favicon.ico" />

  {% for cssFile in cssFiles %}
  <link rel="stylesheet" href="{{ cssFile.file }}" />{{ CRLF }}{{ TAB }}
  {% endfor %}

  {% for jsFile in jsFiles %}
  <script src="{{ jsFile.file }}"></script>{{ CRLF }}{{ TAB }}
  {% endfor %}

  <script>
    //<![CDATA[
      {{ jsData|raw }}

      // reports
      $(function()
      {
        {% if form_error %} jsBackend.messages.add('danger', {{ 'err.FormError'|trans|addslashes }});{% endif %}
        {% if usingRevision %} jsBackend.messages.add('notice', {{ 'msg.UsingARevision'|trans|addslashes }});{% endif %}
        {% if usingDraft %} jsBackend.messages.add('notice', {{ 'msg.UsingADraft'|trans|addslashes }});{% endif %}
        {% if report %} jsBackend.messages.add('success', {{ reportMessage|addslashes }});{% endif %}
        {% if errorMessage %} jsBackend.messages.add('danger', {{ 'errorMessage'|addslashes }});{% endif %}
      });
    //]]>
  </script>
</head>
