<div class="row fork-module-heading">
  <div class="col-md-12">
    <h2>{{ 'lbl.Import'|trans|capitalize }}</h2>
    <div class="btn-toolbar pull-right">
      <div class="btn-group" role="group">
        {% if showProfilesIndex %}
        <a href="{{ geturl('index' ) }}" class="btn btn-default" title="{{ 'lbl.Cancel'|trans|capitalize }}">
          <span class="glyphicon glyphicon-remove"></span>&nbsp;
          {{ 'lbl.Cancel'|trans|capitalize }}
        </a>
        {% endif %}
        <a href="{{ geturl('export_template' ) }}" class="btn btn-default" title="{{ 'lbl.ExportTemplate'|trans|capitalize }}">
          <span class="glyphicon glyphicon-export"></span>&nbsp;
          {{ 'lbl.ExportTemplate'|trans|capitalize }}
        </a>
      </div>
    </div>
  </div>
</div>
{% form import %}
  <div class="row fork-module-content">
    <div class="col-md-12">
      <div class="form-group">
        <label for="group">{{ 'lbl.Group'|trans|capitalize }}</label>
        {% form_field_error group %} {% form_field group %}
      </div>
      <div class="form-group">
        <label for="file">
          {{ 'lbl.File'|trans|capitalize }}&nbsp;
          <abbr class="glyphicon glyphicon-asterisk" title="{{ 'lbl.RequiredField'|trans|capitalize }}"></abbr>
        </label>
        {% form_field_error file %} {% form_field file %}
      </div>
      <div class="form-group">
        <ul class="list-unstyled">
          <li class="checkbox">
            <label>{% form_field overwrite_existing %} {{ msg.OverwriteExisting }}</label>
          </li>
        </ul>
      </div>
    </div>
  </div>
  <div class="row fork-module-actions">
    <div class="col-md-12">
      <div class="btn-toolbar">
        <div class="btn-group pull-right" role="group">
          <button id="importButton" type="submit" name="add" class="btn btn-primary">{{ 'lbl.Import'|trans|capitalize }}</button>
        </div>
      </div>
    </div>
