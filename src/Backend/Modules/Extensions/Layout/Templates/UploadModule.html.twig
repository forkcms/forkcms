{% include "/Layout/Templates/Head.html.twig" %}
{% include "/Layout/Templates/StructureStartModule.html.twig" %}
<div class="row fork-module-heading">
  <div class="col-md-12">
    <h2>{{ 'lbl.UploadModule'|trans|capitalize }}</h2>
  </div>
</div>
{% if zlibIsMissing %}
<div class="row fork-module-messages">
  <div class="col-md-12">
    <div class="alert alert-danger" role="alert">
      {{ msg.ZlibIsMissing }}
    </div>
  </div>
</div>
{% endif %}
{% if notWritable %}
<div class="row fork-module-messages">
  <div class="col-md-12">
    <div class="alert alert-danger" role="alert">
      {{ msg.ThemesNotWritable }}
    </div>
  </div>
</div>
{% endif %}
{% if not zlibIsMissing %}
{% form upload %}
  <div class="row fork-module-content">
    <div class="col-md-12">
      <div class="form-group">
        <label for="file">
          {{ 'lbl.File'|trans|capitalize }}&nbsp;
          <abbr class="glyphicon glyphicon-asterisk" title="{{ 'lbl.RequiredField'|trans|capitalize }}"></abbr>
        </label>
        {% form_field_error file %} {% form_field file %}
      </div>
    </div>
  </div>
  <div class="row fork-module-actions">
    <div class="col-md-12">
      <div class="btn-toolbar">
        <div class="btn-group pull-right" role="group">
          <button id="importButton" type="submit" name="add" class="btn btn-primary">{{ 'lbl.Install'|trans|capitalize }}</button>
        </div>
      </div>
    </div>
  </div>
{% endform %}
{% endif %}
