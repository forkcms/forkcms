<div id="widgetFaqFeedback" class="panel panel-primary">
  <div class="panel-heading">
    <h2 class="panel-title">
      <a href="{{ geturl('index','faq' ) }}">{{ 'lbl.Faq'|trans|capitalize }}: {{ 'lbl.Feedback'|trans|capitalize }}</a>
    </h2>
  </div>
  {% if faqFeedback %}
  <table class="table table-striped dataGrid">
    <tbody>
      {% for _itr_12 in faqFeedback %}
      <tr class="{cycle:'odd':'even'}">
        <td><a href="{{ _itr_12.full_url }}">{{ _itr_12.text|truncate(150 ) }}</a></td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
  {% endif %}
  {% if not faqFeedback %}
  <div class="panel-body">
    <p>{{ msg.NoFeedback }}</p>
  </div>
  {% endif %}
  <div class="panel-footer">
    <div class="btn-toolbar">
      <div class="btn-group">
        <a href="{{ geturl('index','faq' ) }}" class="btn"><span>{{ 'lbl.AllQuestions'|trans|capitalize }}</span></a>
      </div>
    </div>
  </div>
</div>
