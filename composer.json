{
    "name": "forkcms/forkcms",
    "description": "Fork is an open source CMS that will rock your world.",
    "keywords": [
        "cms",
        "fork",
        "fork cms",
        "content management system",
        "symfony",
        "blog",
        "php"
    ],
    "homepage": "https://www.fork-cms.com/",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-gd": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "ext-spl": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/orm": "^2.8",
        "google/recaptcha": "~1.1",
        "jeroendesloovere/geolocation-php-api": "^2.1",
        "liip/imagine-bundle": "^2.5",
        "symfony/monolog-bundle": "^3.7",
        "codex-team/editor.js": "^2.0",
        "beberlei/doctrineextensions": "^1.2",
        "symfony/webpack-encore-bundle": "^1.7",
        "phpoffice/phpspreadsheet": "^1.12",
        "composer/package-versions-deprecated": "^1.11",
        "symfony/flex": "^2.0",
        "symfony/asset": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/form": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/validator": "^6.0",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "symfony/proxy-manager-bridge": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0",
        "symfony/debug-bundle": "^6.0",
        "symfony/yaml": "^6.0",
        "doctrine/annotations": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/property-access": "^6.0",
        "symfony/property-info": "^6.0",
        "symfony/serializer": "^6.0",
        "symfony/messenger": "^6.0",
        "beberlei/assert": "^3.3",
        "symfony/rate-limiter": "^6.0",
        "symfony/http-client": "^6.0",
        "symfony/templating": "^6.0",
        "pageon/pageon-doctrine-data-grid-bundle": "^0.1.24",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/uid": "6.*",
        "composer/semver": "^3.2",
        "symfony/html-sanitizer": "^6.1.0",
        "scienta/doctrine-json-functions": "^5.0",
        "twig/intl-extra": "^3.3",
        "symfony/expression-language": "6.*",
        "symfony/lock": "6.*",
        "tijsverkoyen/convert-to-junit-xml": "^1.10",
        "sumocoders/deployer-sumo-forkcms": "^0.1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "dev-master as 3.6.0",
        "symfony/phpunit-bridge": "^6.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "symfony/var-dumper": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/maker-bundle": "^1.31",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/stopwatch": "^6.0",
        "symfony/web-profiler-bundle": "^6.0",
        "symfony/browser-kit": "^6.0",
        "friendsoftwig/twigcs": "^6.2"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "support": {
        "forum": "https://fork-cms.herokuapp.com",
        "issues": "https://github.com/forkcms/forkcms/issues"
    },
    "scripts": {
        "test": "symfony php bin/simple-phpunit",
        "phpstan": "APP_DEBUG=1 symfony console cache:warm -q -edev && symfony php -d memory_limit=-1 bin/phpstan -vvv analyze src -c phpstan.neon",
        "phpcs": "symfony php -d memory_limit=-1 bin/phpcs --standard=PSR12 --extensions=php --warning-severity=9 --exclude=Generic.Files.LineLength --report=full --ignore=*/tests/*,*/*.tpl.php \"src\"",
        "post-install-cmd": [],
        "post-update-cmd": [],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "autoload": {
        "psr-4": {
            "ForkCMS\\": "src"
        },
        "exclude-from-classmap": [
            "**/Tests/",
            "**/tests/",
            "**/Test/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ForkCMS\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.*"
        }
    }
}
