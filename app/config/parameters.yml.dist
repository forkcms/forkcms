parameters:
    database.driver:        mysql
    database.host:          <database-host>
    database.port:          <database-port>
    database.name:          <database-name>
    database.user:          <database-user>
    database.password:      <database-password>

    kernel.secret:          <secret>

    fork.debug_email:       <debug-email>
    fork.debug_message:     'Internal error.'
    fork.version:           '4.3.1'

    site.protocol:          <site-protocol>
    site.domain:            <site-domain>
    site.default_title:     <site-default-title>
    site.multilanguage:     <site-multilanguage>
    site.default_language:  <site-default-language>
    site.path_www:          %kernel.root_dir%/..
    site.path_library:      %site.path_www%/library

    action.group_tag:       <action-group-tag>
    action.rights_level:    <action-rights-level>

    mailmotor.mail_engine:  'mailchimp'
    mailmotor.api_key:      xxx # enter your mailchimp api_key here
    mailmotor.list_id:      xxx # enter the mailchimp default list_id here
