<?xml version="1.0" encoding="UTF-8"?>
<module>
	<name>faq</name>
	<version>1.0.0</version>
	<requirements>
		<minimum_version>2.0.0</minimum_version>
	</requirements>
	<description>
		<![CDATA[This module enables you to manage FAQs on your site.]]>
	</description>
	<authors>
		<author>
			<name><![CDATA[Lester Lievens]]></name>
			<url><![CDATA[http://www.twitter.com/ElLesso]]></url>
		</author>
	</authors>
	<events>
		<event application="backend" name="after_add"><![CDATA[Triggered when a faq is added.]]></event>
		<event application="backend" name="after_delete"><![CDATA[Triggered when a faq is deleted.]]></event>
		<event application="backend" name="after_edit"><![CDATA[Triggered when a faq is edited.]]></event>
		<event application="backend" name="after_add_category"><![CDATA[Triggered when a category is added.]]></event>
		<event application="backend" name="after_delete_category"><![CDATA[Triggered when a category is deleted.]]></event>
		<event application="backend" name="after_edit_category"><![CDATA[Triggered when a category is edited.]]></event>
	</events>
</module>
